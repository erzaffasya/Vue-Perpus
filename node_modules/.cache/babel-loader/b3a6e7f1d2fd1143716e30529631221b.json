aD,GAnBY;AAoBbC,YAAU,EAAE;AACVd,UAAM,EAANA,MADU;AAEVC,cAAU,EAAVA,UAFU;AAGVJ,UAAM,EAANA,MAHU;AAIVC,eAAW,EAAXA;AAJU;AApBC,CAAf","names":["SwiperCore","Thumbs","Pagination","Navigation","Swiper","SwiperSlide","use","Layout","PageHeader","appConfig","page","title","meta","name","content","description","data","items","text","href","active","components"],"sourceRoot":"","sources":["C:\\Users\\erzafahmifasya\\Documents\\GitHub\\Vue-Perpus\\src\\views\\advance-ui\\swiper.vue"],"sourcesContent":["<script>\nimport SwiperCore, { Thumbs, Pagination, Navigation } from \"swiper\";\nimport { Swiper, SwiperSlide } from \"swiper/vue\";\nimport \"swiper/swiper-bundle.css\";\n\nSwiperCore.use([Thumbs, Pagination, Navigation]);\n\nimport Layout from \"../../layouts/main.vue\";\nimport PageHeader from \"@/components/page-header\";\nimport appConfig from \"../../../app.config\";\n\nexport default {\n  page: {\n    title: \"Swiper\",\n    meta: [{ name: \"description\", content: appConfig.description }],\n  },\n  data() {\n    return {\n      title: \"Swiper\",\n      items: [\n        {\n          text: \"Advance UI\",\n          href: \"/\",\n        },\n        {\n          text: \"Swiper\",\n          active: true,\n        },\n      ],\n    };\n  },\n  components: {\n    Layout,\n    PageHeader,\n    Swiper,\n    SwiperSlide,\n  },\n};\n</script>\n\n<template>\n  <Layout>\n    <PageHeader :title=\"title\" :items=\"items\" />\n\n    <div class=\"row\">\n      <div class=\"col-xl-4 col-lg-6\">\n        <div class=\"card\">\n          <div class=\"card-header\">\n            <h4 class=\"card-title mb-0\">Default Swiper</h4>\n          </div>\n          <!-- end card header -->\n          <div class=\"card-body\">\n            <p class=\"text-muted\">\n              Use\n              <code>default-swiper</code> class to set a default swiper.\n            </p>\n\n            <!-- Swiper -->\n\n            <swiper class=\"default-swiper rounded\">\n              <swiper-slide>\n                <img src=\"@/assets/images/small/img-1.jpg\" alt class=\"img-fluid\" />\n              </swiper-slide>\n              <swiper-slide>\n                <img src=\"@/assets/images/small/img-2.jpg\" alt class=\"img-fluid\" />\n              </swiper-slide>\n              <swiper-slide>\n                <img src=\"@/assets/images/small/img-3.jpg\" alt class=\"img-fluid\" />\n              </swiper-slide>\n            </swiper>\n          </div>\n          <!-- end card-body -->\n        </div>\n        <!-- end card -->\n      </div>\n      <!--end col-->\n\n      <div class=\"col-xl-4 col-lg-6\">\n        <div class=\"card\">\n          <div class=\"card-header\">\n            <h4 class=\"card-title mb-0\">Navigation & Pagination Swiper</h4>\n          </div>\n          <!-- end card header -->\n          <div class=\"card-body\">\n            <p class=\"text-muted\">\n              Use\n              <code>navigation-swiper</code> class to set a swiper with\n              navigation and pagination.\n            </p>\n            <swiper\n              class=\"navigation-swiper rounded\"\n              :loop=\"true\"\n              :autoplay=\"{\n                delay: 2500,\n                disableOnInteraction: false,\n              }\"\n              :navigation=\"{\n                nextEl: '.swiper-button-next',\n                prevEl: '.swiper-button-prev',\n              }\"\n              :pagination=\"{\n                clickable: true,\n                el: '.swiper-pagination',\n              }\"\n            >\n              <swiper-slide>\n                <img src=\"@/assets/images/small/img-4.jpg\" alt class=\"img-fluid\" />\n              </swiper-slide>\n              <swiper-slide>\n                <img src=\"@/assets/images/small/img-5.jpg\" alt class=\"img-fluid\" />\n              </swiper-slide>\n              <swiper-slide>\n                <img src=\"@/assets/images/small/img-6.jpg\" alt class=\"img-fluid\" />\n              </swiper-slide>\n              <div class=\"swiper-button-next\"></div>\n              <div class=\"swiper-button-prev\"></div>\n              <div class=\"swiper-pagination\"></div>\n            </swiper>\n            <!-- Swiper -->\n          </div>\n          <!-- end card-body -->\n        </div>\n        <!-- end card -->\n      </div>\n      <!--end col-->\n\n      <div class=\"col-xl-4 col-lg-6\">\n        <div class=\"card\">\n          <div class=\"card-header\">\n            <h4 class=\"card-title mb-0\">Pagination Dynamic Swiper</h4>\n          </div>\n          <!-- end card header -->\n          <div class=\"card-body\">\n            <p class=\"text-muted\">\n              Use\n              <code>pagination-dynamic-swiper</code> class to set a dynamic\n              swiper with pagination.\n            </p>\n\n            <!-- Swiper -->\n\n            <swiper\n              class=\"pagination-dynamic-swiper rounded\"\n              :loop=\"true\"\n              :autoplay=\"{\n                delay: 2500,\n                disableOnInteraction: false,\n              }\"\n              :pagination=\"{\n                clickable: true,\n                el: '.swiper-pagination',\n                dynamicBullets: true,\n              }\"\n            >\n              <swiper-slide>\n                <img src=\"@/assets/images/small/img-7.jpg\" alt class=\"img-fluid\" />\n              </swiper-slide>\n              <swiper-slide>\n                <img src=\"@/assets/images/small/img-8.jpg\" alt class=\"img-fluid\" />\n              </swiper-slide>\n              <swiper-slide>\n                <img src=\"@/assets/images/small/img-9.jpg\" alt class=\"img-fluid\" />\n              </swiper-slide>\n              <div class=\"swiper-pagination dynamic-pagination\"></div>\n            </swiper>\n          </div>\n          <!-- end card-body -->\n        </div>\n        <!-- end card -->\n      </div>\n      <!--end col-->\n      <div class=\"col-xl-4 col-lg-6\">\n        <div class=\"card\">\n          <div class=\"card-header\">\n            <h4 class=\"card-title mb-0\">Pagination Fraction Swiper</h4>\n          </div>\n          <!-- end card header -->\n          <div class=\"card-body\">\n            <p class=\"text-muted\">\n              Use\n              <code>pagination-fraction-swiper</code> class to set a\n              fraction swiper with pagination.\n            </p>\n\n            <!-- Swiper -->\n\n            <swiper\n              class=\"pagination-fraction-swiper\"\n              :loop=\"true\"\n              :autoplay=\"{\n                delay: 2500,\n                disableOnInteraction: false,\n              }\"\n              :pagination=\"{\n                clickable: true,\n                el: '.swiper-pagination',\n                type: 'fraction',\n              }\"\n              :navigation=\"{\n                nextEl: '.swiper-button-next',\n                prevEl: '.swiper-button-prev',\n              }\"\n            >\n              <swiper-slide>\n                <img src=\"@/assets/images/small/img-10.jpg\" alt class=\"img-fluid\" />\n              </swiper-slide>\n              <swiper-slide>\n                <img src=\"@/assets/images/small/img-11.jpg\" alt class=\"img-fluid\" />\n              </swiper-slide>\n              <swiper-slide>\n                <img src=\"@/assets/images/small/img-12.jpg\" alt class=\"img-fluid\" />\n              </swiper-slide>\n              <div class=\"swiper-button-next\"></div>\n              <div class=\"swiper-button-prev\"></div>\n              <div class=\"swiper-pagination\"></div>\n            </swiper>\n          </div>\n          <!-- end card-body -->\n        </div>\n        <!-- end card -->\n      </div>\n      <!--end col-->\n      <div class=\"col-xl-4 col-lg-6\">\n        <div class=\"card\">\n          <div class=\"card-header\">\n            <h4 class=\"card-title mb-0\">Pagination Custom Swiper</h4>\n          </div>\n          <!-- end card header -->\n          <div class=\"card-body\">\n            <p class=\"text-muted\">\n              Use\n              <code>pagination-custom-swiper</code> class to set a swiper\n              with custom pagination.\n            </p>\n\n            <!-- Swiper -->\n            <swiper\n              class=\"pagination-custom-swiper rounded\"\n              :loop=\"true\"\n              :autoplay=\"{\n                delay: 2500,\n                disableOnInteraction: false,\n              }\"\n              :pagination=\"{\n                clickable: true,\n                el: '.swiper-pagination',\n                renderBullet: function (index, className) {\n                  return '<span class=' + className + '>' + (index + 1) + '</span>';\n                },\n              }\"\n            >\n              <swiper-slide>\n                <img src=\"@/assets/images/small/img-2.jpg\" alt class=\"img-fluid\" />\n              </swiper-slide>\n              <swiper-slide>\n                <img src=\"@/assets/images/small/img-3.jpg\" alt class=\"img-fluid\" />\n              </swiper-slide>\n              <swiper-slide>\n                <img src=\"@/assets/images/small/img-4.jpg\" alt class=\"img-fluid\" />\n              </swiper-slide>\n              <div class=\"swiper-pagination pagination-custom\"></div>\n            </swiper>\n          </div>\n          <!-- end card-body -->\n        </div>\n        <!-- end card -->\n      </div>\n      <!--end col-->\n      <div class=\"col-xl-4 col-lg-6\">\n        <div class=\"card\">\n          <div class=\"card-header\">\n            <h4 class=\"card-title mb-0\">Pagination Progress Swiper</h4>\n          </div>\n          <!-- end card header -->\n          <div class=\"card-body\">\n            <p class=\"text-muted\">\n              Use\n              <code>pagination-progress-swiper</code> class to set a swiper\n              with progress pagination.\n            </p>\n\n            <!-- Swiper -->\n            <swiper\n              class=\"pagination-progress-swiper rounded\"\n              :loop=\"true\"\n              :autoplay=\"{\n                delay: 2500,\n                disableOnInteraction: false,\n              }\"\n              :pagination=\"{\n                clickable: true,\n                el: '.swiper-pagination',\n                type: 'fraction',\n              }\"\n              :navigation=\"{\n                nextEl: '.swiper-button-next',\n                prevEl: '.swiper-button-prev',\n              }\"\n            >\n              <swiper-slide>\n                <img src=\"@/assets/images/small/img-5.jpg\" alt class=\"img-fluid\" />\n              </swiper-slide>\n              <swiper-slide>\n                <img src=\"@/assets/images/small/img-6.jpg\" alt class=\"img-fluid\" />\n              </swiper-slide>\n              <swiper-slide>\n                <img src=\"@/assets/images/small/img-7.jpg\" alt class=\"img-fluid\" />\n              </swiper-slide>\n              <div class=\"swiper-button-next\"></div>\n              <div class=\"swiper-button-prev\"></div>\n              <div class=\"swiper-pagination\"></div>\n            </swiper>\n          </div>\n          <!-- end card-body -->\n        </div>\n        <!-- end card -->\n      </div>\n      <!--end col-->\n      <div class=\"col-xl-4 col-lg-6\">\n        <div class=\"card\">\n          <div class=\"card-header\">\n            <h4 class=\"card-title mb-0\">Scrollbar Swiper</h4>\n          </div>\n          <!-- end card header -->\n          <div class=\"card-body\">\n            <p class=\"text-muted\">\n              Use\n              <code>pagination-scrollbar-swiper</code> class to set a swiper\n              with scrollbar pagination.\n            </p>\n\n            <!-- Swiper -->\n            <swiper\n              class=\"pagination-scrollbar-swiper rounded\"\n              :loop=\"true\"\n              :autoplay=\"{\n                delay: 2500,\n                disableOnInteraction: false,\n              }\"\n              :scrollbar=\"{\n                el: '.swiper-scrollbar',\n                hide: true,\n              }\"\n              :navigation=\"{\n                nextEl: '.swiper-button-next',\n                prevEl: '.swiper-button-prev',\n              }\"\n            >\n              <swiper-slide>\n                <img src=\"@/assets/images/small/img-8.jpg\" alt class=\"img-fluid\" />\n              </swiper-slide>\n              <swiper-slide>\n                <img src=\"@/assets/images/small/img-9.jpg\" alt class=\"img-fluid\" />\n              </swiper-slide>\n              <swiper-slide>\n                <img src=\"@/assets/images/small/img-10.jpg\" alt class=\"img-fluid\" />\n              </swiper-slide>\n              <div class=\"swiper-button-next\"></div>\n              <div class=\"swiper-button-prev\"></div>\n              <div class=\"swiper-scrollbar\"></div>\n            </swiper>\n          </div>\n          <!-- end card-body -->\n        </div>\n        <!-- end card -->\n      </div>\n      <!--end col-->\n\n      <div class=\"col-xl-4 col-lg-6\">\n        <div class=\"card\">\n          <div class=\"card-header\">\n            <h4 class=\"card-title mb-0\">Vertical Swiper</h4>\n          </div>\n          <!-- end card header -->\n          <div class=\"card-body\">\n            <p class=\"text-muted\">\n              Use\n              <code>vertical-swiper</code> class to set a vertical swiper.\n            </p>\n\n            <!-- Swiper -->\n            <swiper\n              class=\"vertical-swiper rounded\"\n              :loop=\"true\"\n              :direction=\"'vertical'\"\n              :autoplay=\"{\n                delay: 2500,\n                disableOnInteraction: false,\n              }\"\n              :pagination=\"{\n                el: '.swiper-pagination',\n                clickable: true,\n              }\"\n              style=\"height: 324px\"\n            >\n              <swiper-slide>\n                <img src=\"@/assets/images/small/img-11.jpg\" alt class=\"img-fluid\" />\n              </swiper-slide>\n              <swiper-slide>\n                <img src=\"@/assets/images/small/img-12.jpg\" alt class=\"img-fluid\" />\n              </swiper-slide>\n              <swiper-slide>\n                <img src=\"@/assets/images/small/img-1.jpg\" alt class=\"img-fluid\" />\n              </swiper-slide>\n              <div class=\"swiper-pagination\"></div>\n            </swiper>\n          </div>\n          <!-- end card-body -->\n        </div>\n        <!-- end card -->\n      </div>\n      <!--end col-->\n\n      <div class=\"col-xl-4 col-lg-6\">\n        <div class=\"card\">\n          <div class=\"card-header\">\n            <h4 class=\"card-title mb-0\">Mousewheel Control Swiper</h4>\n          </div>\n          <!-- end card header -->\n          <div class=\"card-body\">\n            <p class=\"text-muted\">\n              Use\n              <code>mousewheel-control-swiper</code> class to set a swiper\n              with mousewheel scroll.\n            </p>\n\n            <!-- Swiper -->\n            <swiper\n              class=\"mousewheel-control-swiper rounded\"\n              :loop=\"true\"\n              :direction=\"'vertical'\"\n              :mousewheel=\"true\"\n              :autoplay=\"{\n                delay: 2500,\n                disableOnInteraction: false,\n              }\"\n              :pagination=\"{\n                el: '.swiper-pagination',\n                clickable: true,\n              }\"\n              style=\"height: 324px\"\n            >\n              <swiper-slide>\n                <img src=\"@/assets/images/small/img-3.jpg\" alt class=\"img-fluid\" />\n              </swiper-slide>\n              <swiper-slide>\n                <img src=\"@/assets/images/small/img-4.jpg\" alt class=\"img-fluid\" />\n              </swiper-slide>\n              <swiper-slide>\n                <img src=\"@/assets/images/small/img-5.jpg\" alt class=\"img-fluid\" />\n              </swiper-slide>\n              <div class=\"swiper-pagination\"></div>\n            </swiper>\n          </div>\n          <!-- end card-body -->\n        </div>\n        <!-- end card -->\n      </div>\n      <!--end col-->\n\n      <div class=\"col-xl-4 col-lg-6\">\n        <div class=\"card\">\n          <div class=\"card-header\">\n            <h4 class=\"card-title mb-0\">Effect Fade Swiper</h4>\n          </div>\n          <!-- end card header -->\n          <div class=\"card-body\">\n            <p class=\"text-muted\">\n              Use\n              <code>effect-fade-swiper</code> class to set a swiper with\n              fade effect.\n            </p>\n\n            <!-- Swiper -->\n            <swiper\n              class=\"effect-fade-swiper rounded\"\n              :loop=\"true\"\n              :effect=\"'fade'\"\n              :autoplay=\"{\n                delay: 2500,\n                disableOnInteraction: false,\n              }\"\n              :pagination=\"{\n                el: '.swiper-pagination',\n                clickable: true,\n              }\"\n            >\n              <swiper-slide>\n                <img src=\"@/assets/images/small/img-6.jpg\" alt class=\"img-fluid\" />\n              </swiper-slide>\n              <swiper-slide>\n                <img src=\"@/assets/images/small/img-7.jpg\" alt class=\"img-fluid\" />\n              </swiper-slide>\n              <swiper-slide>\n                <img src=\"@/assets/images/small/img-8.jpg\" alt class=\"img-fluid\" />\n              </swiper-slide>\n              <div class=\"swiper-pagination\"></div>\n            </swiper>\n          </div>\n          <!-- end card-body -->\n        </div>\n        <!-- end card -->\n      </div>\n      <!--end col-->\n      <div class=\"col-xl-4 col-lg-6\">\n        <div class=\"card\">\n          <div class=\"card-header\">\n            <h4 class=\"card-title mb-0\">Effect Creative Swiper</h4>\n          </div>\n          <!-- end card header -->\n          <div class=\"card-body\">\n            <p class=\"text-muted\">\n              Use\n              <code>effect-creative-swiper</code> class to set a swiper with\n              creative custom effect.\n            </p>\n\n            <!-- Swiper -->\n            <swiper\n              class=\"effect-creative-swiper rounded\"\n              :loop=\"true\"\n              :effect=\"'creative'\"\n              :creativeEffect=\"{\n                prev: {\n                  shadow: true,\n                  translate: [0, 0, -400],\n                },\n                next: {\n                  translate: ['100%', 0, 0],\n                },\n              }\"\n              :autoplay=\"{\n                delay: 2500,\n                disableOnInteraction: false,\n              }\"\n              :pagination=\"{\n                el: '.swiper-pagination',\n                clickable: true,\n                dynamicBullets: true,\n              }\"\n            >\n              <swiper-slide>\n                <img src=\"@/assets/images/small/img-9.jpg\" alt class=\"img-fluid\" />\n              </swiper-slide>\n              <swiper-slide>\n                <img src=\"@/assets/images/small/img-10.jpg\" alt class=\"img-fluid\" />\n              </swiper-slide>\n              <swiper-slide>\n                <img src=\"@/assets/images/small/img-11.jpg\" alt class=\"img-fluid\" />\n              </swiper-slide>\n              <div class=\"swiper-pagination\"></div>\n            </swiper>\n          </div>\n          <!-- end card-body -->\n        </div>\n        <!-- end card -->\n      </div>\n      <!--end col-->\n      <div class=\"col-xl-4 col-lg-6\">\n        <div class=\"card\">\n          <div class=\"card-header\">\n            <h4 class=\"card-title mb-0\">Effect Flip Swiper</h4>\n          </div>\n          <!-- end card header -->\n          <div class=\"card-body\">\n            <p class=\