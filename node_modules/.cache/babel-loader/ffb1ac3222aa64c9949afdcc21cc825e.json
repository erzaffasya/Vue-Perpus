jC,EAAiC,IAAjC,EAAiCmB,iBAA1BH,IAAI,CAACI,aAAqB,CAAjC,EAAyB;AAAA;AAAzB,SAJkD,EAKlDpB,oBAAwC,IAAxC,cAAwCmB,iBAAnBH,IAAI,CAACK,MAAc,CAAxC,EAAgC;AAAA;AAAhC,SALkD,EAMlDC,WANkD,CAApD;OAaK,CAbL;;AAAA,OAcM,EAfR,CAiBM,EADNX,gCACM,CA7BR,CA8BI,CA/BN,CAmDI,EAnBJX,oBAkBM,KAlBN,eAkBM,CAjBJA,oBAgBM,KAhBN,eAgBM,CAfyDQ,cAAI,mBAAjEI,oBAEI,GAFJ,EAEI;cAAA;AAFDd,aAAK,EAAC,oCAEL;AAF0CI,YAAI,EAAC,GAE/C;AAFqEqB,eAAK;AAAA,iBAAEf,YAAF;AAAA;AAE1E,OAFJ,EAAwF,YAAxF,sCAeI,EAZJR,oBAQK,IARL,eAQK,oBAPHY,oBAMKC,SANL,EAMK,IANL,EAMKC,YAH4BN,YAAMgB,KAAN,CAAYhB,aAAI,CAAhB,EAAsBA,aAAI,CAA1B,CAG5B,EAHsD,UAAhDiB,UAAgD,EAApCR,KAAoC,EAA/B;6BAH5BL,oBAMK,IANL,EAMK;AANAd,eAAK;oBAA8B2B,UAAU,IAAIjB,UAA5C;sBAA8EiB,UAAU;AAAxF,YAML;AAH8DP,aAAG,EAAED,KAGnE;AAFFM,iBAAK;AAAA,mBAAEf,aAAOiB,UAAT;AAAA;AAEH,SANL,GAKEzB,oBAA6C,GAA7C,eAA6CmB,iBAAjBM,UAAiB,CAA7C,EAAsC;AAAA;AAAtC,UALF;;AAAA;OAMK,CANL;;AAAA,OAOG,EARL,CAYI,EAHgEjB,aAAOA,YAAMkB,wBAAjFd,oBAEI,GAFJ,EAEI;cAAA;AAFDd,aAAK,EAAC,2BAEL;AAFiCI,YAAI,EAAC,GAEtC;AAF2CqB,eAAK;AAAA,iBAAEf,YAAF;AAAA;AAEhD,OAFJ,EAAyF,QAAzF,sCAGI,CAhBN,CAiBI,CAlBN,CAmBI,CApDN,CAqDI,CAtDN,CAuDI,CAxDN,CAD4C,CAA5C;AAAA,MA0DO;;;;AAAA,GA3DT","names":["class","id","_createElementVNode","scope","href","_createBlock","_component_Layout","_createVNode","_component_PageHeader","title","$data","items","_hoisted_6","_createCommentVNode","_createElementBlock","_Fragment","_renderList","$options","data","index","key","_toDisplayString","nama_kategori","detail","_hoisted_10","onClick","slice","pageNumber","length"],"sourceRoot":"","sources":["C:\\Users\\erzafahmifasya\\Documents\\GitHub\\Vue-Perpus\\src\\views\\kategori\\lihat.vue"],"sourcesContent":["<script>\nimport Layout from \"../../layouts/main.vue\";\nimport PageHeader from \"@/components/page-header\";\nimport appConfig from \"../../../app.config\";\nimport apiKategori from \"../../apis/Kategori.js\";\n\nexport default {\n  page: {\n    title: \"Grid Js\",\n    meta: [{ name: \"description\", content: appConfig.description }],\n  },\n  mounted() {\n    apiKategori.lihatKategori().then(response => {\n      this.data = response.data.data;\n      \n        this.setPages();\n    })\n      .catch(() => {\n\n      });\n  },\n\n  data() {\n    return {\n      title: \"Grid Js\",\n      items: [\n        {\n          text: \"Tables\",\n          href: \"/\",\n        },\n        {\n          text: \"Grid Js\",\n          active: true,\n        },\n      ],\n      data: [],\n      page: 1,\n      perPage: 5,\n      pages: [],\n      totalData:0\n    };\n  },\n  name: \"Widgets\",\n  components: {\n    Layout,\n    PageHeader,\n  },\n  computed: {\n    displayedPosts() {\n      return this.paginate(this.data);\n    },\n    resultQuery() {\n      if (this.searchQuery) {\n        const search = this.searchQuery.toLowerCase();\n        return this.displayedPosts.filter((data) => {\n          return (\n            data.id.toLowerCase().includes(search) ||\n            data.nama_kategori.toLowerCase().includes(search) ||\n            data.detail.toLowerCase().includes(search) \n          );\n        });\n      } else {\n        return this.displayedPosts;\n      }\n    },\n  },\n  watch: {\n    posts() {\n      this.setPages();\n    },\n  },\n  created() {\n    // this.setPages();\n  },\n  filters: {\n    trimWords(value) {\n      return value.split(\" \").splice(0, 20).join(\" \") + \"...\";\n    },\n  },\n  methods: {\n    setPages() {\n      console.log();\n      let numberOfPages = Math.ceil(this.data.length / this.perPage);\n      for (let index = 1; index <= numberOfPages; index++) {\n        this.pages.push(index);\n      }\n    },\n    paginate(data) {\n      let page = this.page;\n      let perPage = this.perPage;\n      let from = page * perPage - perPage;\n      let to = page * perPage;\n      return data.slice(from, to);\n    },\n  },\n};\n</script>\n\n<template>\n  <Layout>\n    <PageHeader :title=\"title\" :items=\"items\" />\n    <div class=\"row\">\n      <div class=\"card\">\n        <div class=\"card-body\" id=\"hello\">\n          <div class=\"table-responsive table-card\">\n            <table class=\"table align-middle table-nowrap\" id=\"customerTable\">\n              <thead class=\"table-light text-muted\">\n                <tr>\n                  <th class=\"sort\" data-sort=\"currency_name\" scope=\"col\">ID</th>\n                  <th class=\"sort\" data-sort=\"current_value\" scope=\"col\">\n                    Nama Kategori\n                  </th>\n                  <th class=\"sort\" data-sort=\"pairs\" scope=\"col\">Detail</th>\n                  <th scope=\"col\">Action</th>\n                </tr>\n              </thead>\n              <!--end thead-->\n              <tbody class=\"list form-check-all\">\n                <tr v-for=\"(data, index) of resultQuery\" :key=\"index\">\n                  <td class=\"id\">\n                    {{ data.id }}\n                  </td>\n                  <td>{{ data.nama_kategori }}</td>\n                  <td class=\"pairs\">{{ data.detail }}</td>\n                  <td>\n                    <span\n                      ><a href=\"#\" class=\"text-reset text-decoration-underline\"\n                        >Details</a\n                      ></span\n                    >\n                  </td>\n                </tr>\n              </tbody>\n              <!--end tbody-->\n            </table>\n          </div>\n          <div class=\"d-flex justify-content-end mt-3\">\n            <div class=\"pagination-wrap hstack gap-2\">\n              <a class=\"page-item pagination-prev disabled\" href=\"#\" v-if=\"page != 1\" @click=\"page--\">\n                Previous\n              </a>\n              <ul class=\"pagination listjs-pagination mb-0\">\n                <li :class=\"{\n                  active: pageNumber == page,\n                  disabled: pageNumber == '...',\n                }\" v-for=\"(pageNumber, index) in pages.slice(page - 1, page + 5)\" :key=\"index\"\n                  @click=\"page = pageNumber\">\n                  <a class=\"page\" href=\"#\">{{ pageNumber }}</a>\n                </li>\n              </ul>\n              <a class=\"page-item pagination-next\" href=\"#\" @click=\"page++\" v-if=\"page < pages.length\">\n                Next\n              </a>\n            </div>\n          </div>\n        </div>\n      </div>\n    </div>\n  </Layout>\n</template>\n"]}]}