e: 0,\n        },\n        colors: [\"#e9ebec\", \"#11d1b7\", \"#695eef\"],\n        xaxis: {\n            categories: [\"2014\", \"2015\", \"2016\", \"2017\", \"2018\", \"2019\"],\n        },\n    },\n};\n\n// Polygon - Radar Charts\n\nconst polygonRadarChart = {\n    series: [{\n        name: \"Series 1\",\n        data: [20, 100, 40, 30, 50, 80, 33],\n    }, ],\n    chartOptions: {\n        chart: {\n            height: 350,\n            type: \"radar\",\n            toolbar: {\n                show: false,\n            },\n        },\n        dataLabels: {\n            enabled: true,\n        },\n        plotOptions: {\n            radar: {\n                size: 140,\n            },\n        },\n        title: {\n            text: \"Radar with Polygon Fill\",\n            style: {\n                fontWeight: 500,\n            },\n        },\n        colors: [\"#81e0eb\"],\n        markers: {\n            size: 4,\n            colors: [\"#fff\"],\n            strokeColor: \"#81DFEA\",\n            strokeWidth: 2,\n        },\n        tooltip: {\n            y: {\n                formatter: function(val) {\n                    return val;\n                },\n            },\n        },\n        xaxis: {\n            categories: [\n                \"Sunday\",\n                \"Monday\",\n                \"Tuesday\",\n                \"Wednesday\",\n                \"Thursday\",\n                \"Friday\",\n                \"Saturday\",\n            ],\n        },\n        yaxis: {\n            tickAmount: 7,\n            labels: {\n                formatter: function(val, i) {\n                    if (i % 2 === 0) {\n                        return val;\n                    } else {\n                        return \"\";\n                    }\n                },\n            },\n        },\n    },\n};\n\nexport { basicRadarChart, radarMultiseriesChart, polygonRadarChart };"]}]}