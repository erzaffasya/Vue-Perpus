"","sourcesContent":["<script>\nimport { reactive, ref } from \"vue\";\nimport { useRouter } from \"vue-router\";\nimport apiKategori from \"../../apis/Kategori.js\";\n\nimport Layout from \"../../layouts/main.vue\";\nimport PageHeader from \"@/components/page-header\";\nimport appConfig from \"../../../app.config\";\n\nexport default {\n  page: {\n    title: \"Layouts\",\n    meta: [{ name: \"description\", content: appConfig.description }],\n  },\n  \n  components: {\n    Layout,\n    PageHeader,\n  },\n\n  setup() {\n    const kategori = reactive({\n      nama_kategori: \"\",\n      detail: \"\",\n    });\n\n    const validation = ref([]);\n    const router = useRouter();\n    function store() {\n      console.log('erza')\n      apiKategori\n        .tambahKategori(kategori)\n        .then(() => {\n          router.push({\n            name: \"lihat-kategori\",\n          });\n        })\n        .catch((err) => {\n          validation.value = err.response.data.error;\n        });\n    }\n    return {\n      kategori,\n      validation,\n      router,\n      store,\n    };\n  },\n}; \n</script>\n\n<template>\n  <Layout>\n    <PageHeader :title=\"title\" :items=\"items\" />\n    <div class=\"row\">\n      <div class=\"col-xxl-6\">\n        <div class=\"card\">\n          <div class=\"card-header align-items-center d-flex\">\n            <h4 class=\"card-title mb-0 flex-grow-1\">Horizontal Form</h4>\n          </div>\n          <!-- end card header -->\n          <div class=\"card-body\">\n            <p class=\"text-muted\">\n              Create horizontal forms with the grid by adding the\n              <code>row</code> class to form groups and using the\n              <code>col-*-*</code> class to specify the width of your labels and\n              controls. Be sure to add <code>col-form-label</code> class to your\n              <code>&lt;label&gt;</code>s as well so theyâ€™re vertically centered\n              with their associated form controls.\n            </p>\n            <div class=\"live-preview\">\n              <form @submit.prevent=\"store()\">\n                <div class=\"row mb-3\">\n                  <div class=\"col-lg-3\">\n                    <label for=\"nameInput\" class=\"form-label\"\n                      >Nama Kategori</label\n                    >\n                  </div>\n                  <div class=\"col-lg-9\">\n                    <input\n                      type=\"text\"\n                      v-model=\"kategori.nama_kategori\"\n                      class=\"form-control\"\n                      id=\"nameInput\"\n                      placeholder=\"Enter your name\"\n                    />\n                  </div>\n                </div>\n                <div class=\"row mb-3\">\n                  <div class=\"col-lg-3\">\n                    <label for=\"nameInput\" class=\"form-label\">Detail</label>\n                  </div>\n                  <div class=\"col-lg-9\">\n                    <input\n                      type=\"text\"\n                      v-model=\"kategori.detail\"\n                      class=\"form-control\"\n                      id=\"nameInput\"\n                      placeholder=\"Enter your name\"\n                    />\n                  </div>\n                </div>\n                <div class=\"text-end\">\n                  <button type=\"submit\" class=\"btn btn-primary\">\n                    Add Leave\n                  </button>\n                </div>\n              </form>\n            </div>\n            <div class=\"d-none code-view\"></div>\n          </div>\n        </div>\n      </div>\n    </div>\n  </Layout>\n</template>\n"]}]}