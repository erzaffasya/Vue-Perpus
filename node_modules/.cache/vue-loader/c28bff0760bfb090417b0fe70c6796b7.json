kers: {\n            width: 20,\n            height: 6,\n            radius: 2,\n          },\n          itemMargin: {\n            horizontal: 12,\n            vertical: 0,\n          },\n        },\n        colors: [\"#8ee3ed\", \"#8a82f3\"],\n        tooltip: {\n          y: [\n            {\n              formatter: function (y) {\n                if (typeof y !== \"undefined\") {\n                  return y.toFixed(0) + \"k\";\n                }\n                return y;\n              },\n            },\n          ],\n        },\n      },\n    };\n  },\n};\n</script>\n\n<template>\n  <div class=\"card card-height-100\">\n    <div class=\"card-header align-items-center d-flex\">\n      <h4 class=\"card-title mb-0 flex-grow-1\">Audiences Sessions by Country</h4>\n      <div class=\"flex-shrink-0\">\n        <div class=\"dropdown card-header-dropdown\">\n          <a\n            class=\"text-reset dropdown-btn\"\n            href=\"#\"\n            data-bs-toggle=\"dropdown\"\n            aria-haspopup=\"true\"\n            aria-expanded=\"false\"\n          >\n            <span class=\"fw-semibold text-uppercase fs-12\">Sort by: </span\n            ><span class=\"text-muted\"\n              >Current Week<i class=\"mdi mdi-chevron-down ms-1\"></i\n            ></span>\n          </a>\n          <div class=\"dropdown-menu dropdown-menu-end\">\n            <a class=\"dropdown-item\" href=\"#\">Today</a>\n            <a class=\"dropdown-item\" href=\"#\">Last Week</a>\n            <a class=\"dropdown-item\" href=\"#\">Last Month</a>\n            <a class=\"dropdown-item\" href=\"#\">Current Year</a>\n          </div>\n        </div>\n      </div>\n    </div>\n    <!-- end card header -->\n    <div class=\"card-body p-0\">\n      <div>\n        <apexchart\n          class=\"apex-charts\"\n          height=\"390\"\n          dir=\"ltr\"\n          :series=\"series\"\n          :options=\"chartOptions\"\n        ></apexchart>\n      </div>\n    </div>\n    <!-- end c