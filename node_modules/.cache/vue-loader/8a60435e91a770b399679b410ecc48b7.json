        const card = check.closest('.card');\n                const preview = card.querySelector(\".live-preview\");\n                const code = card.querySelector(\".code-view\");\n                if (check.checked) {\n                    // do this\n                    preview.classList.add(\"d-none\");\n                    code.classList.remove(\"d-none\");\n                } else {\n                    // do that\n                    preview.classList.remove(\"d-none\");\n                    code.classList.add(\"d-none\");\n                }\n            });\n        });\n    }\n\n};\n</script>\n\n<template>\n    <Layout>\n        <PageHeader :title=\"title\" :items=\"items\" />\n        <div class=\"row\">\n            <div class=\"col-xl-6\">\n                <div class=\"card\">\n                    <div class=\"card-header align-items-center d-flex\">\n                        <h4 class=\"card-title mb-0 flex-grow-1\">Ratio Video 16:9</h4>\n                        <div class=\"flex-shrink-0\">\n                            <div class=\"form-check form-switch form-switch-right form-switch-md\">\n                                <label\n                                    for=\"ratiovideo1-showcode\"\n                                    class=\"form-label text-muted\"\n                                >Show Code</label>\n                                <input\n                                    class=\"form-check-input code-switcher\"\n                                    type=\"checkbox\"\n                                    id=\"ratiovideo1-showcode\"\n                                />\n                            </div>\n                        </div>\n                    </div>\n                    <!-- end card header -->\n                    <div class=\"card-body\">\n                        <p class=\"text-muted\">\n                            Wrap any embed in\n                            <code>&lt;iframe&gt;</code> tag, in a parent element, use\n                            <code>ratio-16x9</code> class to set aspect ratio 16:9.\n                        </p>\n                        <div class=\"live-preview\">\n                            <!-- 16:9 aspect ratio -->\n                            <div class=\"ratio ratio-16x9\">\n                                <iframe\n                                    class=\"rounded\"\n                                    src=\"https://www.youtube.com/embed/1y_kfWUCFDQ\"\n                                    title=\"YouTube video\"\n                                    allowfullscreen\n                                ></iframe>\n                            </div>\n                        </div>\n                        <div class=\"d-none code-view\">\n                            <pre class=\"language-markup\">\n<code>&lt;!-- Ratio Video 16:9 --&gt;\n&lt;div class=&quot;ratio ratio-16x9&quot;&gt;\n&lt;iframe src=&quot;https://www.youtube.com/embed/1y_kfWUCFDQ&quot; title=&quot;YouTube video&quot; allowfullscreen&gt;&lt;/iframe&gt;\n&lt;/div&gt;</code></pre>\n                        </div>\n                    </div>\n                    <!-- end card-body -->\n                </div>\n                <!-- end card -->\n\n                <div class=\"card\">\n                    <div class=\"card-header align-items-center d-flex\">\n                        <h4 class=\"card-title mb-0 flex-grow-1\">Ratio Video 4:3</h4>\n                        <div class=\"flex-shrink-0\">\n                            <div class=\"form-check form-switch form-switch-right form-switch-md\">\n                                <label\n                                    for=\"ratiovideo3-showcode\"\n                                    class=\"form-label text-muted\"\n                                >Show Code</label>\n                                <input\n                                    class=\"form-check-input code-switcher\"\n                                    type=\"checkbox\"\n                                    id=\"ratiovideo3-showcode\"\n                                />\n                            </div>\n                        </div>\n                    </div>\n                    <!-- end card header -->\n                    <div class=\"card-body\">\n                        <p class=\"text-muted\">\n                            Use\n                            <code>ratio-4x3</code> class to set aspect ratio 4:3.\n                        </p>\n                        <div class=\"live-preview\">\n                            <!-- 4:3 aspect ratio -->\n                            <div class=\"ratio ratio-4x3\">\n                                <iframe\n                                    class=\"rounded\"\n                                    src=\"https://www.youtube.com/embed/PHcgN1GTjdU\"\n                                    title=\"YouTube video\"\n                                    allowfullscreen\n                                ></iframe>\n                            </div>\n                        </div>\n                        <div class=\"d-none code-view\">\n                            <pre class=\"language-markup\">\n<code>&lt;!-- Ratio Video 4:3 --&gt;\n&lt;div class=&quot;ratio ratio-4x3&quot;&gt;\n&lt;iframe src=&quot;https://www.youtube.com/embed/1y_kfWUCFDQ&quot; title=&quot;YouTube video&quot; allowfullscreen&gt;&lt;/iframe&gt;\n&lt;/div&gt;</code></pre>\n                        </div>\n                    </div>\n                    <!-- end card-body -->\n                </div>\n                <!-- end card -->\n\n                <div class=\"card\">\n                    <div class=\"card-header align-items-center d-flex\">\n                        <h4 class=\"card-title mb-0 flex-grow-1\">Custom Ratios</h4>\n                        <div class=\"flex-shrink-0\">\n                            <div class=\"form-check form-switch form-switch-right form-switch-md\">\n                                <label\n                                    for=\"ratiovideocustom-showcode\"\n                                    class=\"form-label text-muted\"\n                                >Show Code</label>\n                                <input\n                                    class=\"form-check-input code-switcher\"\n                                    type=\"checkbox\"\n                                    id=\"ratiovideocustom-showcode\"\n                                />\n                            </div>\n                        </div>\n                    </div>\n                    <!-- end card header -->\n                    <div class=\"card-body\">\n                        <p class=\"text-muted\">\n                            Use\n                            <code>--vz-aspect-ratio: 50%</code> to style element to set aspect ratio 2:1.\n                        </p>\n                        <div class=\"live-preview\">\n                            <!-- 16:9 aspect ratio -->\n                            <div class=\"ratio\" style=\"--vz-aspect-ratio: 50%;\">\n                                <iframe\n                                    class=\"rounded\"\n                                    src=\"https://www.youtube.com/embed/2RZQN_ko0iU\"\n                                    title=\"YouTube video\"\n                                    allowfullscreen\n                                ></iframe>\n                            </div>\n                        </div>\n                        <div class=\"d-none code-view\">\n                            <pre class=\"language-markup\">\n<code>&lt;!-- Custom Ratio Video --&gt;\n&lt;div class=&quot;ratio&quot; style=&quot;--vz-aspect-ratio: 50%;&quot;&gt;\n&lt;iframe src=&quot;https://www.youtube.com/embed/2RZQN_ko0iU&quot; title=&quot;YouTube video&quot; allowfullscreen&gt;&lt;/iframe&gt;\n&lt;/div&gt;</code></pre>\n                        </div>\n                    </div>\n                    <!-- end card-body -->\n                </div>\n                <!-- end card -->\n            </div>\n            <!-- end col -->\n\n            <div class=\"col-xl-6\">\n                <div class=\"card\">\n                    <div class=\"card-header align-items-center d-flex\">\n                        <h4 class=\"card-title mb-0 flex-grow-1\">Ratio Video 21:9</h4>\n                        <div class=\"flex-shrink-0\">\n                            <div class=\"form-check form-switch form-switch-right form-switch-md\">\n                                <label\n                                    for=\"ratiovideo2-showcode\"\n                                    class=\"form-label text-muted\"\n                                >Show Code</label>\n                                <input\n                                    class=\"form-check-input code-switcher\"\n                                    type=\"checkbox\"\n                                    id=\"ratiovideo2-showcode\"\n                                />\n                            </div>\n                        </div>\n                    </div>\n                    <!-- end card header -->\n                    <div class=\"card-body\">\n                        <p class=\"text-muted\">\n                            Use\n                            <code>ratio-21x9</code> class to set aspect ratio 21:9.\n                        </p>\n                        <div class=\"live-preview\">\n                            <!-- 21:9 aspect ratio -->\n                            <div class=\"ratio ratio-21x9\">\n                                <iframe\n                                    class=\"rounded\"\n                                    src=\"https://www.youtube.com/embed/Z-fV2lGKnnU\"\n                                    title=\"YouTube video\"\n                                    allowfullscreen\n                                ></iframe>\n                            </div>\n                        </div>\n                        <div class=\"d-none code-view\">\n                            <pre class=\"language-markup\">\n<code>&lt;!-- Ratio Video 21:9 --&gt;\n&lt;div class=&quot;ratio ratio-21x9&quot;&gt;\n&lt;iframe src=&quot;https://www.youtube.com/embed/Z-fV2lGKnnU&quot; title=&quot;YouTube video&quot; allowfullscreen&gt;&lt;/iframe&gt;\n&lt;/div&gt;</code></pre>\n                        </div>\n                    </div>\n                    <!-- end card-body -->\n                </div>\n                <!-- end card -->\n\n                <div class=\"card\">\n                    <div class=\"card-header align-items-center d-flex\">\n                        <h4 class=\"card-title mb-0 flex-grow-1\">Ratio Video 1:1</h4>\n                        <div class=\"flex-shrink-0\">\n                            <div class=\"form-check form-switch form-switch-right form-switch-md\">\n                                <label\n                                    for=\"ratiovideo4-showcode\"\n                                    class=\"form-label text-muted\"\n                                >Show Code</label>\n                                <input\n                                    class=\"form-check-input code-switcher\"\n                                    type=\"checkbox\"\n                                    id=\"ratiovideo4-showcode\"\n                                />\n                            </div>\n                        </div>\n                    </div>\n                    <!-- end card header -->\n                    <div class=\"card-body\">\n                        <p class=\"text-muted\">\n                            Use\n                            <code>ratio-1x1</code> class to set aspect ratio 1:1.\n                        </p>\n                        <div class=\"live-preview\">\n                            <!-- 1:1 aspect ratio -->\n                            <div class=\"ratio ratio-1x1\">\n                                <iframe\n                                    class=\"rounded\"\n                                    src=\"https://www.youtube.com/embed/GfSZtaoc5bw\"\n                                    title=\"YouTube video\"\n                                    allowfullscreen\n                                ></iframe>\n                            </div>\n                        </div>\n                        <div class=\"d-none code-view\">\n                            <pre class=\"language-markup\">\n<code>&lt;!-- Ratio Video 1:1 --&gt;\n&lt;div class=&quot;ratio ratio-1x1&quot;&gt;\n&lt;iframe src=&quot;https://www.youtube.com/embed/GfSZtaoc5bw&quot; title=&quot;YouTube video&quot; allowfullscreen&gt;&lt;/iframe&gt;\n&lt;/div&gt;</code></pre>\n                        </div>\n                    </div>\n                    <!-- end card-body -->\n                </div>\n                <!-- end card -->\n            </div>\n            <!-- end col -->\n        </div>\n        <!-- end row -->\n    </Layout>\n</template>"]}]}